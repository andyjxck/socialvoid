{
  "expo": {
    "name": "Game Void",
    "slug": "game-void",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "bundleIdentifier": "com.createinc.6a9a88925dfd40fd95d12c13e2b26983",
      "buildNumber": "1",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "xyz.create.CreateExpoEnvironment"
    },

    "plugins": [
      ["expo-router", { "sitemap": false }],
      ["expo-splash-screen", { "image": "./assets/images/splash-icon.png", "imageWidth": 200, "resizeMode": "contain" }],
      "expo-audio",
      ["expo-build-properties", { "ios": { "useFrameworks": "static" } }],
      "expo-video",
      "expo-font",
      "expo-secure-store",
      "expo-web-browser"
    ],

    "web": { "bundler": "metro", "favicon": "./assets/images/favicon.png" },
    "experiments": { "typedRoutes": true },
    "extra": { "router": { "origin": false } },
    "runtimeVersion": { "policy": "appVersion" }
  }
}
